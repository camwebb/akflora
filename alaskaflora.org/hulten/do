#!/usr/bin/gawk -f 

@load "xml"

BEGIN{

  XMLMODE=0
  XMLCHARSET="utf-8"

  
  # READ QUERY STRING
  split(ENVIRON["QUERY_STRING"], qs, "&")
  for (q in qs) {
    split(qs[q], qp, "=")
    f[qp[1]] = substr(urldecode(qp[2]),1,100)
  }

  # SEARCH
  if ((f["method"] == "search") && (f["q"])) {
    
    header("Search results")
    print "<h1>Search results</h1>"
    print "<p>Query = <i>" f["q"] "</i><p>"

    # Read data
    PROCINFO["sorted_in"] = "@ind_str_asc"
  
    XMLMODE = 1
    while ((getline < "hulten.xml") > 0) {
      # attributes
      if (XMLSTARTELEM == "taxon")
        id = XMLATTR["id"]
      # gather cdata
      else if (XMLEVENT == "CHARDATA")
        cdata = $0
      # assign cdata to variables
      else if (XMLENDELEM == "name")
        name = cdata
      else if (XMLENDELEM == "fam")
        fam = cdata
      else if (XMLENDELEM == "xy")
        outc++
      # end of record
      else if (XMLENDELEM == "taxon") {
        # test
        if ((tolower(name) ~ tolower(f["q"]) ) || \
            (tolower(fam) ~ tolower(f["q"]) ) ) {
          out[name] = id
          # for (i in syn)
          #   print "  " i
          outnc[name] = outc
        }
        outc = 0
        delete syn
      }
    }
    XMLMODE = 0

    if (length(out)) {
      print "<table>"
      print "<tr><th>Name</th><th>Collections</th><tr>"
      for (i in out)
        print "<tr><td><a href=\"do?method=detail&amp;id=" out[i] "\">" i "</a></td><td>" outnc[i] "</td></tr>"
      print "</table>"
    }
    else
      print "<p>No results</p>"

    footer()
  }

  # DETAILS
  else if ((f["method"] == "detail") && f["id"]) {

    XMLMODE = 1
    while ((getline < "hulten.xml") > 0) {
      # attributes
      if (XMLSTARTELEM == "taxon")
        id = XMLATTR["id"]
      # gather cdata
      else if (XMLEVENT == "CHARDATA")
        cdata = $0
      # assign cdata to variables
      else if (XMLENDELEM == "name")
        name = cdata
      else if (XMLENDELEM == "syn")
        syn[cdata]++
      else if (XMLENDELEM == "fam")
        fam = cdata
      else if (XMLENDELEM == "common")
        common = cdata
      else if (XMLENDELEM == "desc")
        desc = cdata
      else if (XMLENDELEM == "ecol")
        ecol = cdata
      else if (XMLENDELEM == "tax")
        tax = cdata
      else if (XMLENDELEM == "use")
        use = cdata
      else if (XMLENDELEM == "xy")
        xy[cdata]++
      # end of record
      else if (XMLENDELEM == "taxon") {
        # test
        if (id == f["id"]) {
          outname = name
          outcommon = common
          outfam = fam
          outdesc = desc
          outecol = ecol
          outtax = tax
          outuse = use
          for (i in syn)
            outsyn = outsyn ", " i
          for (i in xy)
            outxy = outxy " " gensub(/^([^,]+),(.*)$/, \
              "L.circle([\\2, \\1], attr).addTo(map); ","G", i)
        }
        delete syn
        delete xy
        id = name = common = fam = desc = ecol = tax = use = ""
      }
    }
    XMLMODE = 0

    header("Hultén: " outname)
    
    # print "<h1>Object details</h1>"

    print "<table>"
    print "<tr><td>&#160;</td><td style=\"font-weight:bold; font-size: 1.2em;\">" outname "</td></tr>"

    print "<tr><td>&#160;</td><td>"
    print "<table style=\"margin-bottom: 0px;\"><tr><td>"
    print "<div style=\"padding-left: 10px; height: 270px; width:190px;border: 1px solid #BBB; border-radius: 4px;\"><a href=\"https://alaskaflora.org/img/hulten/" f["id"] ".jpg\" target=\"_blank\"><img src=\"https://alaskaflora.org/img/hulten/" f["id"] ".jpg\" style=\"display: block; margin-left: auto; margin-right: auto; max-height: 95%; max-width: 95%;\"/></a></div>"
    print "</td><td>"
    print "<div style=\"border: 1px solid #BBB; border-radius: 4px; margin-left: 10px; padding: 10px;\"><div id=\"map\" style=\"height: 250px; width: 250px;\"></div></div>"
    print "</td></tr></table>"
    print "</td></tr>"
    
    if (outfam)
      print "<tr><td><i>Family</i></td><td>" outfam "</td></tr>"
    if (outsyn)
      print "<tr><td style=\"vertical-align: top;\"><i>Synonyms</i></td><td>" gensub(/^, /,"","G",outsyn)  "</td></tr>"
    if (outcommon)
      print "<tr><td><i>Common&#160;name</i></td><td>" outcommon "</td></tr>"
    if (outdesc)
      print "<tr><td style=\"vertical-align: top;\"><i>Description</i></td><td>" outdesc "</td></tr>"
    if (outecol)
      print "<tr><td style=\"vertical-align: top;\"><i>Ecology</i></td><td>" outecol "</td></tr>"
    if (outtax)
      print "<tr><td style=\"vertical-align: top;\"><i>Taxonomy<br/>notes</i></td><td>" outtax "</td></tr>"
    if (outuse)
      print "<tr><td style=\"vertical-align: top;\"><i>Uses</i></td><td>" outuse "</td></tr>"
    
    print"</table>"
    
    print "<script type=\"text/javascript\">"
    print "  var map = L.map('map', { zoomControl: false }).setView([63, -150], 3);"
    print "  L.tileLayer('https://{s}.tile.opentopomap.org/{z}/{x}/{y}.png', { attribution: '<a href=\"https://www.openstreetmap.org/copyright\">OSM</a>, <a href=\"http://viewfinderpanoramas.org\">SRTM</a>, <a href=\"https://opentopomap.org\">OTM</a>' }).addTo(map);"
    print "  var attr = { color: 'blue', radius: 10000 };"
    #print "  L.circle([63, -150], attr).addTo(map);"
    if (outxy)
      print outxy
    print "</script>"

    footer()
    
  }

  else if (f["method"] == "about") {
    
    header("Flora of Alaska")

    print "<h1>Digitizing Hultén’s Flora</h1>"

    print "<div style=\"text-align: center; margin-bottom: 2em;\"><img src=\"../img/hultenpage.png\" alt=\"Hulten example\" style=\"margin-left: auto; margin-right: auto; display:block; width:80%;\"/><i style=\"line-height: 1em;\">The standardized layout of a taxon account</i></div>"
    
    print "<p>The digitization steps were:</p>"
    print "<ol>"
    print "<li>Scanning. We used <a href=\"https://datelinedigitalprinting.com/\">Dateline Printing</a> in Fairbanks, who have a Xerox Color 550. A copy of the book was sacrificed (the covers and spine were removed), and the pages were fed into the sheet feeder. The positioning of each page was quite consistent, which helped greatly with sectioning the page elements (below).</li>"
    print "<li>Sectioning. Using a combination of <a href=\"https://imagemagick.org/\">Imagemagick</a> tools and <a href=\"https://github.com/akflora/akflora/blob/master/dataflow/Hulten/hulten2info\">scripted algorithms</a>, each page image was divided into subimages: one or two taxon accounts and/or identification keys, then each account into illustration, text, and two maps (see image above). About 25% of pages required some manual intervention.</li>"
    print "<li>OCR. Each text block was processed with <a href=\"https://github.com/tesseract-ocr/tesseract\">tesseract</a> to extract text, using a comprehensive <a href=\"../pages/blog7.html\">list</a> of expected genus and species names as input to increase spelling accuracy of scientific names.</li><li>Text sectioning. In a text editor, the raw OCR text was manually divided into six paragraphs: taxon names, synonyms, description, ecology, taxonomy notes, and uses.</li>"
    print "<li>Specimen point extraction.  The image of the Alaska specimen distribution map was processed (<a href=\"../pages/blog10.html\">see details</a>) to extract coordinates for each specimen dot.</li><li>Collection. Scripts then scanned appropriate directories and files to check for a known set of likely errors, and then assembled the text elements and point locations into a single XML file.</li>"
    print "</ol>"

    print "<p>Steps 2-4 required manual input, and were executed page-by-page, working through the book. Overall the quality of the OCR has been good, but some spelling errors remain. Misspelled taxonomic names will be reconciled to our <a href=\"../pages/blog7.html\">canonical list</a> of name before Hultén’s data is incorporated into the new online flora.</p>"

    footer()
  }

  else {

    header("Flora of Alaska")

    print "<script type=\"text/javascript\">"    \
      "function showhide() { "                          \
      "var x = document.getElementById(\"genera\"); " \
      "var y = document.getElementById(\"button\"); "   \
      "  if (x.style.display == \"none\") { " \
      "    x.style.display = \"block\"; " \
      "    y.innerHTML = \"Hide...\"; " \
      "  } else { " \
      "    x.style.display = \"none\"; " \
      "    y.innerHTML = \"Family and Genus list...\"; " \
      "  } " \
      "} " \
      "</script>"
    
    print "<h1>Hultén’s Flora of Alaska</h1>"
    # print "<p>Search:</p>"
    print "<form action=\"do\">"
    print "<input type=\"hidden\" name=\"method\" value=\"search\"/>"
    print "<table>"
    print "<tr><td>Search names:</td><td>" \
      "<input type=\"text\" name=\"q\"/>&#160;<input type=\"submit\" value=\"Submit\"/></td></tr>"
    print "</table>"
    #  print "<p>Search terms: plain text, or <a href=\"https://www.gnu.org/software/gawk/manual/gawk.html#Regexp\">regular expression</a>.<br/>Entering both Description and Material terms implies AND.</p><br/>"
    print "</form>"

    print "<button id=\"button\" onclick=\"showhide()\">Family and Genus list...</button>"
    print "<div id=\"genera\" style=\"display:none;\">"
    print "<p style=\"border: 1px solid #BBB; border-radius: 4px; padding: 20px; font-size: 1.5rem;\">"
    genlist()
    print "</p>"
    print "</div>"

    footer()
    
  }
    
  exit 1
}

